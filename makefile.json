{
	"basic.newCompilation":{
		"prefix": "new Compilation",
		"body": [
			"${1:main}: $1.c",
				"\tgcc -Wall -std=c11 -o $1 $1.c"
			],
		"description": "generates new compilation LL Style"
	},
	"basic.newCompilationForDebug":{
		"prefix": "new Compilation debug",
		"body": [
			"${1:main}: $1.c",
				"\tgcc -Wall -std=c11 -g -o $1 $1.c"
		]
	},
	"basic.newCompilationSignal":{
		"prefix": "new Compilation for signal",
		"body": [
			"${1:main}: $1.c",
				"\tgcc -Wall -std=c11 -D_POSIX_SOURCE -o $1 $1.c"
			],
		"description": "generates new compilation LL Style for signal"
	},
}